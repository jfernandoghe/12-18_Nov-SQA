language: python
python:
  - "2.7"

install:
  - pip install flake8
  - pip install autopep8
  - sudo apt install pep8
script:
  - flake8 --max-complexity=2 python_lab.py
  - python python_lab.py -m coverage --with-coverage --cover-min-percentage=99
  - pep8 python_lab.py
  - python -m doctest python_lab_2.py  
